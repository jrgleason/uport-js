{
  "name": "uport",
  "version": "0.6.2",
  "description": "Library for interacting with uport profiles and attestations",
  "main": "lib/index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "tutorial"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build:es5": "./node_modules/.bin/babel src -d lib",
    "build:browser": "./node_modules/.bin/webpack",
    "build": "./node_modules/.bin/yarn build:es5 && ./node_modules/.bin/yarn build:browser",
    "build:docs:html": "./node_modules/.bin/jsdoc src/* README.md -d docs",
    "build:docs:md": "./node_modules/jsdoc-to-markdown/bin/cli.js -f src/* --template 'template.hbs' -c js2doc.conf > docs/reference/index.md; echo",
    "build:docs": "npm run build:docs:html && npm run build:docs:md",
    "prepublish": "./node_modules/.bin/yarn build"
  },
  "author": "Pelle Braendgaard <pelle.braendgaard@consensys.net>",
  "license": "MIT",
  "dependencies": {
    "base64url": "^3.0.0",
    "ethjs-util": "github:jrgleason/ethjs-util#UPGRADE",
    "jsontokens": "^0.7.6",
    "mnid": "^0.1.1",
    "nets": "^3.2.0",
    "tweetnacl": "^1.0.0",
    "tweetnacl-util": "^0.15.0",
    "uport-lite": "^1.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/nock"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "ajv": "^6.5.1",
    "babel-loader": "^8.0.0-beta.3",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "jest": "^23.1.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "json-loader": "^0.5.4",
    "mockdate": "^2.0.1",
    "nock": "^9.0.6",
    "webpack": "^4.12.0",
    "yarn": "^1.7.0"
  }
}
